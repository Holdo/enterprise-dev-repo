{
  "_version": "1.3.0",
  "sap.app": {
    "_version": "1.3.0",
    "id": "RootFolder.repository",
    "type": "application",
    "title": "Enterprise Development Repository",
    "applicationVersion": {
      "version": "1.0.0"
    }
  },
  "sap.ui": {
    "_version": "1.3.0",
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    },
    "supportedThemes": ["sap_bluecrystal"],
    "fullWidth": true
  },
  "sap.ui5": {
    "_version": "1.1.0",
    "rootView": "RootFolder.repository.App",
    "dependencies": {
      "minUI5Version": "1.36.12",
      "libs": {
        "sap.m": { }
      }
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "RootFolder.repository",
        "controlId": "app",
        "controlAggregation": "pages",
        "transition": "slide"
      },
      "routes": [{
        "pattern": "",
        "name": "appHome",
        "target": "home"
      }],
      "targets": {
        "home": {
          "viewName": "Launchpad",
          "viewLevel" : 1
        }
      }
    }
  }
}